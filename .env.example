# Local contracts directory
# Use absolute path for MCP clients or relative path for local development
OPENAPI_CONTRACT_DIR=./contracts

# S3 Configuration (optional)
# Set S3_ENABLED=true to enable S3 storage
# When enabled, provide AWS credentials and S3 bucket details
S3_ENABLED=false
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key
# S3_BUCKET=my-openapi-contracts
# S3_PREFIX=openapi-contracts

# Debug logging
# Set to true or 1 to enable debug output
DEBUG_MCP=false
